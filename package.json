    "react-router-dom": "4.3.1",
    "react-router-redux": "5.0.0-alpha.9",
    "react-typist": "^2.0.4",
    "react-vis": "^1.9.2",
    "reactcss": "^1.2.3",
    "redux": "4.0.0",
    "redux-form": "^7.3.0",
    "redux-form-antd": "^1.0.5",
    "redux-immutable": "4.0.0",
    "redux-persist": "^5.9.1",
    "redux-saga": "0.16.0",
    "redux-thunk": "^2.3.0",
    "reselect": "3.0.1",
    "sanitize.css": "4.1.0",
    "serve-favicon": "^2.4.5",
    "socket.io": "^2.0.4",
    "socket.io-client": "^2.1.1",
    "spotify-web-api-node": "^3.1.1",
    "styled-components": "3.3.2",
    "uuid": "^3.3.2",
    "voucher-code-generator": "^1.1.1",
    "warning": "4.0.1",
    "web3": "^1.0.0-beta.36",
    "whatwg-fetch": "2.0.4",
    "winston": "^3.0.0"
  },
